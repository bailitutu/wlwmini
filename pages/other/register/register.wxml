<view class='body g-tc'>

    <view class='_find-type g-m-t-40'>
        <view class='_find-type-content g-m-t-40'>
            <van-button size="small" round type="info" custom-class='g-bg-blue  g-c-white _type-btn' bind:click='handlePhone'>企业</van-button>
            <van-button size="small" round type="info" custom-class='g-bg-gray  g-c-white _type-btn' bind:click='handlePhone'>个人</van-button>
        </view>
    </view>

    <view class='g-pd-lr-60 g-m-t-60'>
       <van-cell-group  border='{{ false }}'>
              <view class='g-fs-28 g-c-gray g-tl g-pd-20'><text class='g-c-red'>* </text>用户名</view>
            <van-field
                type='number'
                value="{{ value }}"
                placeholder="请输入手机号"
                border="{{ true }}"
                bind:change="changePhone"
            />
                   <view class='g-fs-28 g-c-gray g-tl g-pd-20'><text class='g-c-red'>* </text>设置密码</view>
            <van-field
                type='number'
                value="{{ value }}"
                placeholder="请输入手机号"
                border="{{ true }}"
                bind:change="changePhone"
            />
                   <view class='g-fs-28 g-c-gray g-tl g-pd-20'><text class='g-c-red'>* </text> 确认密码</view>
            <van-field
                type='number'
                value="{{ value }}"
                placeholder="请输入手机号"
                border="{{ true }}"
                bind:change="changePhone"
            />
            <view class='g-fs-28 g-c-gray g-tl g-pd-20'><text class='g-c-red'>* </text>手机号</view>
            <van-field
                type='number'
                value="{{ value }}"
                placeholder="请输入手机号"
                border="{{ true }}"
                bind:change="changePhone"
            />
             <view class='g-fs-28 g-c-gray g-tl  g-pd-20'><text class='g-c-red'>* </text>验证码</view>
            <van-field
                value="{{ value }}"
                placeholder="请输入验证码"
                border="{{ true }}"
                bind:change="changePwdCode"
                use-button-slot
                type='number'
            >
                 <van-button
                    slot="button"
                    plain
                    size="small"
                    round
                    type="info"
                    custom-class='g-c-blue g-b-blue'
                    bind:click='handleSendCode'
                    >
                    发送验证码
                    </van-button>
            </van-field>
            <view class=' g-m-t-60 g-m-b-60'>
                <van-button size="large"  type="info" custom-class='g-bg-blue  g-c-white' bind:click='handlePersonRegister'>注册</van-button>
            </view>

        </van-cell-group>

    </view>
    <view class='g-pd-lr-60 g-m-t-60'>
       <van-cell-group  border='{{ false }}'>
              <view class='g-fs-28 g-c-gray g-tl g-pd-20'><text class='g-c-red'>* </text>公司名称</view>
            <van-field
                type='number'
