            </van-field>
            <view class=' g-m-t-60 g-m-b-60'>
                <van-button size="large"  type="info" custom-class='g-bg-blue  g-c-white' bind:click='handleCompanyRegister'>注册</van-button>
            </view>

        </van-cell-group>

    </view>

</view>

